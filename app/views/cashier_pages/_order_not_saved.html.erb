<div class="dataTables_wrapper" role="grid">
  <table cellpadding="0" cellspacing="0" border="0" class="table table-hover table-bordered datatables" id="table-order-not-saved">
    <thead>
      <tr>
        <th>
          Mã Hoá Đơn
        </th>
        <th>
          Tên
        </th>
        <th>
          Giờ tạo
        </th>
        <th>
          Giờ cập nhật
        </th>
        <th>
          Tổng Tiền
        </th>
        <th style='width: 15%;'>
          <center>
            Trạng Thái
          </center>
        </th>
      </tr>
    </thead>
    <tbody>
      <tr class="not-saved-loading">
        <td colspan="6">
          <center>
            <i class='fa fa-spin fa-spinner' style="margin-right: 5px"></i> Đang tải dữ liệu
          </center>
        </td>
      </tr>
      <tr class="no-not-saved-orders no-orders" style="display: none">
        <td colspan="6">
          <div class="alert alert-warning">Tạm thời không có order mới.</div>
        </td>
      </tr>
      <script type="text/javascript">
      $.ajax({
        type: "GET",
        dataType: "script",
        url: "<%= not_saved_sales_cashier_pages_path %>"
      });
      </script>
    </tbody>
  </table>
</div>
